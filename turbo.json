{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": [
				"app/**/build",
				"examples/**/build",
				"packages/**/build",
				"protocol/build"
			]
		},
		"start": {
			"dependsOn": ["^build"]
		},
		"watch": {
			"dependsOn": ["^build"],
			"cache": false
		},
		"anchor-test": {
			"cache": false
		},
		"shadcn-add": {
			"cache": false
		},
		"supabase-typegen": {
			"cache": false
		},
		"dev": {
			"dependsOn": ["^build"],
			"cache": false
		}
	},
	"globalDotEnv": [".env"],
	"globalDependencies": [".env"]
}
