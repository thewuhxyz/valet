{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["app/**/build", "packages/**/build"]
		},
		"watch": {
			"dependsOn": ["^build"],
			"cache": false
    },
		"dev": {
			"dependsOn": ["^build"],
			"cache": false
    }
	},
	"globalDependencies": [".env"]
}
