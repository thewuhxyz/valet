{
	"name": "@valet/token",
	"version": "0.1.0",
	"module": "dist/esm/index.js",
	"main": "dist/cjs/index.js",
	"source": "src/index.ts",
	"types": "dist/esm/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js"
		}
	},
	"browser": "./dist/browser/index.js",
	"scripts": {
		"build-bro": "rollup --config",
		"build-browser-js": "node build.js && pnpm ts-types",
		"build-browser": "ts-node --project ./tsconfig.browser.json build.ts && pnpm ts-types",
		"build-cjs": "tsc -p ./tsconfig.cjs.json && echo '{ \"type\": \"commonjs\" }' > dist/cjs/package.json",
		"build-esm": "tsc -p ./tsconfig.esm.json",
		"build-all": "pnpm build-esm && pnpm build-browser && pnpm build-cjs",
		"build": "pnpm build-esm && pnpm build-cjs",
		"ts-types": "tsc -p ./tsconfig.browser.json --emitDeclarationOnly",
		"watch": "esbuild dist/browser/index.js --bundle --platform=browser --format=esm --outdir=browser --watch"
	},
	"dependencies": {
		"@coral-xyz/anchor": "^0.28.0",
		"@coral-xyz/spl-token": "0.28.1-beta.1",
		"@metaplex-foundation/beet": "^0.7.2",
		"@metaplex-foundation/mpl-token-metadata": "^2.7.0",
		"@solana/buffer-layout": "^4.0.1",
		"@solana/spl-token": "^0.3.9",
		"@solana/spl-token-registry": "^0.2.4574",
		"@solana/web3.js": "1.88.0",
		"@valet/lib": "workspace:*",
		"bn.js": "^5.2.1",
		"bs58": "^5.0.0"
	},
	"devDependencies": {
		"@rollup/plugin-alias": "^5.1.0",
		"@rollup/plugin-commonjs": "^25.0.7",
		"@rollup/plugin-json": "^6.1.0",
		"@rollup/plugin-node-resolve": "^15.2.3",
		"@rollup/plugin-typescript": "^11.1.6",
		"@types/bn.js": "^5.1.5",
		"@types/chrome": "^0.0.254",
		"@types/node": "^20.10.4",
		"crypto-browserify": "^3.12.0",
		"esbuild": "^0.19.10",
		"esbuild-node-builtins": "^0.1.0",
		"esbuild-plugin-d.ts": "^1.2.3",
		"esbuild-plugin-tsc": "^0.4.0",
		"npm-dts": "^1.3.12",
		"rollup": "^4.13.0",
		"rollup-plugin-node-polyfills": "^0.2.1",
		"rollup-plugin-polyfill-node": "^0.13.0",
		"rollup-plugin-terser": "^7.0.2",
		"stream-browserify": "^3.0.0",
		"ts-node": "^10.9.2",
		"typescript": "~4.9.3"
	},
	"peerDependencies": {
		"@metaplex-foundation/umi": "^0.8.2"
	}
}